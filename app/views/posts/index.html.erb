<h3>スケジュール一覧</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>更新日</th>
      <th>確認</th>
      <th>削除</th>
    </tr>
  </thead>
  <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td><%= post.id %></td>
            <td><%= post.title %></td>
            <td><%= post.startday %></td>
            <td><%= post.endday %></td>
            <td><%= post.allday %></td>
            <td><%= post.content %></td>
            <td><%= post.updated_at %></td>
          </tr>
        <% end %>
      </tbody>
</table>
<p><%= link_to "スケジュール新規作成", :new_post %></P>
